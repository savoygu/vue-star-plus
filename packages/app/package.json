{
  "name": "vue-star-plus",
  "version": "3.0.0",
  "description": "Plus version of vue-star component",
  "author": "savoygu",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/vue-star-plus.umd.js",
  "module": "./dist/vue-star-plus.es.js",
  "typings": "./types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-star-plus.es.js",
      "require": "./dist/vue-star-plus.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:savoygu/vue-star-plus.git"
  },
  "keywords": [
    "vue-star",
    "vue-star-plus"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savoygu/vue-star-plus/issues"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "sass": "^1.52.1",
    "sass-bem": "^2.6.5",
    "sass-config": "^1.1.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0",
    "vue-tsc": "^0.34.7"
  }
}